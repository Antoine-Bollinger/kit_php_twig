{
    "name": "abollinger/partez",
    "description": "A simple & fast PHP starter kit for web app",
    "type": "project",
    "require": {
        "twig/twig": "^3.8",
        "twig/html-extra": "^3.8",
        "twig/intl-extra": "^3.8",
        "vlucas/phpdotenv": "^5.6",
        "abollinger/helpers": "^1.2"
    },
    "require-dev": {
        "abollinger/pret": "^1.6"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Abollinger\\Partez\\": "src/",
            "Abollinger\\Bin\\": "bin/"
        }
    },
    "scripts": {
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "php -S localhost:1234 -t public/"
        ],
        "partez": [
            "php vendor/bin/pret partez directory=public"
        ],
        "post-create-project-cmd": [
            "Abollinger\\Bin\\Logs::InstallationComplete"
        ]
    },
    "authors": [
        {
            "name": "Antoine Bollinger",
            "email": "antoine.bollinger@gmail.com"
        }
    ]
}