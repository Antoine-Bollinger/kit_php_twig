{
    "name": "abollinger/partez",
    "description": "A simple & fast PHP starter kit for web app",
    "type": "project",
    "require": {
        "twig/twig": "^3.8",
        "twig/html-extra": "^3.8",
        "twig/intl-extra": "^3.8",
        "vlucas/phpdotenv": "^5.6",
        "abollinger/helpers": "^1.2",
        "abollinger/bricolo": "^1.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Abollinger\\Partez\\": "src/"
        }
    },
    "scripts": {
        "serve": "php bricolo serve d=public",
        "post-create-project-cmd": "php bricolo log m=afterinstall"
    },
    "config": {
        "process-timeout": 0
    },
    "authors": [
        {
            "name": "Antoine Bollinger",
            "email": "antoine.bollinger@gmail.com",
            "homepage": "https://antoinebollinger.fr",
            "role": "Developer"
        }
    ]
}